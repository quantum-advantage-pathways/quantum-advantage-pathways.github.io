    "sections": [
      {
        "title": "Understanding Quantum Chemistry Simulations",
        "type": "text",
        "content": "Quantum chemistry simulations leverage quantum computers to model molecular structures and interactions with unprecedented accuracy. This leaderboard tracks the performance of various quantum and classical algorithms in simulating molecular properties."
      },
      {
        "title": "Scoring System",
        "type": "cards",
        "cards": [
          {
            "title": "Accuracy Measurement",
            "content": "Accuracy is measured by comparing to FCI (Full Configuration Interaction) calculations, which provide the exact solution within the chosen basis set."
          },
          {
            "title": "Time Measurement",
            "content": "Time includes circuit preparation, execution, and post-processing. For quantum hardware, this includes error mitigation time."
          },
          {
            "title": "Hardware Categories",
            "content": "Results are categorized by hardware type: quantum processors (superconducting, trapped ion, etc.) or classical computers (CPU, GPU, TPU)."
          }
        ]
      }
    ]
